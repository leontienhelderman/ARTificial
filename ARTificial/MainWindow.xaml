<Window x:Name="file" x:Class="ARTificial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ARTificial"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="filebutton" Content="Choose file" HorizontalAlignment="Left" Margin="157,178,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Image x:Name="image" HorizontalAlignment="Left" Height="100" Margin="487,178,0,0" VerticalAlignment="Top" Width="100"/>
        <Image x:Name="image1" HorizontalAlignment="Left" Height="100" Margin="452,178,0,0" VerticalAlignment="Top" Width="100"/>
        <TextBlock x:Name="file1" HorizontalAlignment="Left" Margin="157,262,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Text="Press button to open file"/>

    </Grid>
</Window>
